var count = 0;
var limit = 10;

print "Using 'while' loop:";

// Loops 10 times; count goes from 0 to 9.
while count < limit {
    print count;
    count = count + 1;
}
print "";

print "Using 'for' loop:";

// Loops 10 times; i goes from 0 to 9.
for var i = 0; i < limit; i = i + 1 {
    print i;
}
print "";

print "Using 'for' loop in reverse:";

// Loops 10 times; i goes from 10 to 1.
for var i = limit; i > 0; i = i - 1 {
    print i;
}

print i;    // runtime error
